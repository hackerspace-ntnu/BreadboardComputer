nop   -> 00000
mv    -> 00001
li    -> 00010
ld    -> 00011
ldind -> 00100
ldio  -> 00101
stio  -> 00110
add   -> 00111
sub   -> 01000
neg   -> 01001
xor   -> 01010
nand  -> 01011
and   -> 01100
or    -> 01101
not   -> 01110
j     -> 01111
jnz   -> 10000
jimm  -> 10001
addi  -> 10010
st    -> 10011
jz    -> 10100
jn    -> 10101